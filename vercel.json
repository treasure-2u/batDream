{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "/proxy_plants/service/varietyInfo/varietyList",
      "dest": "https://api.example.com",
      "methods": ["GET"]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
